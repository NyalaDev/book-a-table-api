    "lint": "eslint --fix \"./**/*.js\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "api-doc": "swagger-markdown -i ./extensions/documentation/documentation/1.0.0/full_documentation.json -o API_DOCS.md"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "swagger-markdown": "^1.4.3"
  },
  "dependencies": {
    "faker": "^5.4.0",
    "knex": "<0.20.0",
    "sqlite3": "5.0.0",
    "strapi": "3.5.0",
    "strapi-admin": "3.5.0",
    "strapi-connector-bookshelf": "3.5.0",
    "strapi-plugin-content-manager": "3.5.0",
    "strapi-plugin-content-type-builder": "3.5.0",
    "strapi-plugin-documentation": "3.5.0",
    "strapi-plugin-email": "3.5.0",
    "strapi-plugin-upload": "3.5.0",
    "strapi-plugin-users-permissions": "3.5.0",
    "strapi-utils": "3.5.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
